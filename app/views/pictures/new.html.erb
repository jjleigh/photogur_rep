
<div class="row">
	<div class="large-6 medium-centered columns">
		<div class="new-edit-form">
			<h1 class="text-center user-head">Add a picture</h1>
			<hr>
		
			<%= form_for @picture do |f|%>

				<p>
					<%= f.label "artist" %><br>
					<%= f.text_field "artist" %>
				</p>

				<p>
					<%= f.label "title" %><br>
					<%= f.text_field "title" %>
				</p>

				<p>
					<%= f.label "url" %><br>
					<%= f.text_field "url" %>
				</p>

				<p>
					<%= f.submit "save", :class => 'button' %>
				</p>
			<% end %>
		</div>
		
	</div>
</div>
 